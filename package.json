{
  "name": "react-rubick",
  "version": "0.0.1",
  "description": "Alauda front end project Rubick built with Vue stack + TypeScript, just for study.",
  "repository": "git@github.com:JounQin/react-rubick.git",
  "author": "JounQin <admin@1stg.me>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "dev": "cross-env DEBUG=rubick:* NODE_ENV=development ts-node server",
    "build": "rimraf dist && webpack --config build/webpack.config.ts --colors --progress",
    "lint": "tslint -p . -t stylish",
    "precommit": "lint-staged",
    "sync:push": "bash build/sync-push.sh"
  },
  "lint-staged": {
    "*.ts": [
      "tslint -p . -t stylish",
      "git add"
    ],
    "*.{json,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "all",
    "overrides": [
      {
        "files": "package.json",
        "options": {
          "printWidth": 150
        }
      }
    ]
  },
  "dependencies": {
    "create-react-context": "^0.2.2",
    "koa": "^2.5.1",
    "koa-compose": "^4.1.0",
    "lodash": "^4.17.10",
    "qs": "^6.5.2",
    "react": "^16.4.0",
    "react-async-component": "^2.0.0",
    "react-dom": "^16.4.0",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2"
  },
  "devDependencies": {
    "@types/debug": "^0.0.30",
    "@types/html-webpack-plugin": "^2.30.3",
    "@types/koa": "^2.0.46",
    "@types/koa-compose": "^3.2.2",
    "@types/koa-webpack": "^5.0.0",
    "@types/lodash": "^4.14.109",
    "@types/node": "^10.3.1",
    "@types/prop-types": "^15.5.3",
    "@types/qs": "^6.5.1",
    "@types/react": "^16.3.16",
    "@types/react-dom": "^16.0.6",
    "@types/react-router": "^4.0.26",
    "@types/react-router-config": "^1.0.7",
    "@types/react-router-dom": "^4.2.7",
    "@types/webpack": "^4.4.0",
    "@types/webpack-env": "^1.13.6",
    "cross-env": "^5.1.6",
    "debug": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "koa-webpack": "^5.0.2",
    "lint-staged": "^7.1.3",
    "lodash-es": "^4.17.10",
    "prettier": "^1.13.4",
    "prop-types": "^15.6.1",
    "rimraf": "^2.6.2",
    "ts-loader": "^4.3.1",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-eslint-rules": "^5.3.1",
    "tslint-plugin-prettier": "^1.3.0",
    "tslint-react": "^3.6.0",
    "typescript": "^2.9.1",
    "webpack": "^4.11.0",
    "webpack-cli": "^3.0.2"
  }
}
