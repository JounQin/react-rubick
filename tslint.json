{
  "extends": [
    "tslint:recommended",
    "tslint-react",
    "tslint-eslint-rules",
    "tslint-config-prettier"
  ],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "interface-name": false,
    "member-access": false,
    "no-console": [true, "log"],
    "no-empty": [true, "allow-empty-catch"],
    "object-literal-sort-keys": false,
    "ordered-imports": [
      true,
      {
        "named-imports-order": "lowercase-last"
      }
    ],
    "prettier": true
  }
}
